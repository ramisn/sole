<%= favorite_feed_item_title(feed_item) %>
<div class="mtm product-item">
  <%= render :partial => "feed_items/product_images", 
  :locals => {:products => [feed_item.feedable.product]+feed_item.consolidated_feed_items.limit(2).inject([]) do |array, item| 
                if item.consolidated
                  array << item.consolidated.product
                end 
                array
              end}%>
</div>
<div class="options">
  <%= link_to "See all favorites", favorites_member_collection_path(feed_item.feedable.user) %>
  <%= link_to "Comment", "#comments", :class => "show-comments" %>
</div>
